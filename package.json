{
  "name": "svg-chameleon",
  "version": "0.4.0",
  "description": "A node module used for creating a customizable SVG sprite, containing SVGs as symbols that can be customized in the frontend using CSS variables and more.",
  "main": "src/index.ts",
  "bin": "./dist/cli.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc && node dist/index.js",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christian-reichart/svg-chameleon"
  },
  "keywords": [
    "svg",
    "sprite"
  ],
  "author": "Christian Reichart",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "svg-sprite": "^1.5.0",
    "svgo": "^1.3.2",
    "svgson": "^4.1.0",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@types/svg-sprite": "0.0.32",
    "@types/svgo": "^1.3.3",
    "@types/yargs": "^15.0.9",
    "nodemon": "^2.0.5",
    "typescript": "^4.0.5"
  }
}
